      "cell_type": "code",
      "source": [
        "def first_occurrence(arr, x):\n",
        "    low, high = 0, len(arr) - 1\n",
        "    ans = -1\n",
        "    while low <= high:\n",
        "        mid = (low + high) // 2\n",
        "        if arr[mid] == x:\n",
        "            ans = mid\n",
        "            high = mid - 1\n",
        "        elif arr[mid] < x:\n",
        "            low = mid + 1\n",
        "        else:\n",
        "            high = mid - 1\n",
        "    return ans\n",
        "\n",
        "\n",
        "def last_occurrence(arr, x):\n",
        "    low, high = 0, len(arr) - 1\n",
        "    ans = -1\n",
        "    while low <= high:\n",
        "        mid = (low + high) // 2\n",
        "        if arr[mid] == x:\n",
        "            ans = mid\n",
        "            low = mid + 1\n",
        "        elif arr[mid] < x:\n",
        "            low = mid + 1\n",
        "        else:\n",
        "            high = mid - 1\n",
        "    return ans\n",
        "\n",
        "\n",
        "T = int(input())\n",
        "for _ in range(T):\n",
        "    N, Q = map(int, input().split())\n",
        "    arr = list(map(int, input().split()))\n",
        "\n",
        "    query_values = list(map(int, input().split())) # Read all Q query values from a single line\n",
        "    for x in query_values: # Iterate through each query value\n",
        "        first = first_occurrence(arr, x)\n",
        "        if first == -1:\n",
        "            print(0)\n",
        "        else:\n",
        "            last = last_occurrence(arr, x)\n",
        "            print(last - first + 1)"
      ],
